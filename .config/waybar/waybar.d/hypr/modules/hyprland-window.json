{
  "hyprland/window": {
    "format": "{title}",
    "rewrite": {
        "(.*) — Mozilla Firefox": "$1",
        "Firefox Developer Edition": "$1",
        "(.*) — Firefox Developer Edition": "$1",
        "(.*) - Visual Studio Code": "$1",
        "(.*) - Visual Studio Code - Insiders": "$1",
        "(.*) - Quod Libet": "$1",
        "nvim (.*)": "$1",
        "(.*) - Betterbird": "$1",
	"(.*)༽(.*)": "$1$2",
	"(.*?)༽(.*)": "$1$2"
    },
    "icon": true,
    "icon-size": 16,
    "max-length": 100,
    "separate-outputs": true
  }
}
